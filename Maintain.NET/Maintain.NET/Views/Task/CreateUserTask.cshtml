@model Maintain.NET.Models.UserMaintenanceTask

@{
    Layout = "_Layout";
}

<p>add task</p>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MaintenanceTask" class="control-label"></label>
                <select asp-for="MaintenanceTask" class="form-control" asp-items="ViewBag.UserTaskID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Add" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back</a>
</div>